package  {		public class Item {				private var name:String;		private var numItem:int;		private var description:String;				public function Item(nm:String, n:int) {			name = nm;			numItem = n;		}				//Getters and setters		public function getName():String{			return name;		}		public function getNum():int{			return numItem;		}		public function setNum(n:int):void{			numItem = n;		}				public function decrement():void{			numItem--;		}		public function increment():void{			numItem++;		}	}	}