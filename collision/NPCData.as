package collision{		public class NPCData {		public var costume:int;		public var npcX:int;		public var npcY:int;		public var npcWidth:int		public var npcHeight:int		public var speech:Array		public var afterSpeech:Array		public var hasTalkedToMe:Boolean		public var giftGiven:Boolean		public var afterGiftSpeech:Array		public function NPCData(npcCost:int, xPos:int, yPos:int, w:int, h:int, hT:Boolean, s:Array, s2:Array = null, gG:Boolean = false, s3:Array = null):void{			costume = npcCost;			npcX = xPos;			npcY = yPos;			npcWidth = w;			npcHeight = h;			speech = s;			afterSpeech = s2;			hasTalkedToMe = hT;						giftGiven = gG;			afterGiftSpeech = s3;		}	}	}