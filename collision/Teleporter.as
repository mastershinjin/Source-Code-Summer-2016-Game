package collision {		import flash.events.*;		public class Teleporter extends BoxCollider{		private var linkedEvent:GameEvent;		private var isTeleported:Boolean;		private var destination:String;		public function Teleporter(gE:GameEvent, d:String){						super(0, 0, 0, 0);						linkedEvent = gE;			destination = d;						addEventListener(Event.ENTER_FRAME, checkIfTeleport);		}		//Teleports to destination if event is true		public function checkIfTeleport(event:Event):void{			if(linkedEvent.getEvent()){								_root.prevArea = _root.currentLabel;				_root.gotoAndStop(destination);								//TEMP set event back to false				linkedEvent.setEvent(false);								removeEventListener(Event.ENTER_FRAME, checkIfTeleport);			}		}	}	}