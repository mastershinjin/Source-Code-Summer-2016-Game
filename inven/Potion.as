package inven{		public class Potion extends Item{		private var hp:int;		private var sp:int;		private var canRevive:Boolean;		//Constructor		public function Potion(nm:String, n:int, h:int, s:int, rev:Boolean) {			super(nm, n);			hp = h;			sp = s;			canRevive = rev;		}				public function getHPRecv():int{			return hp;		}		public function getSPRecv():int{			return sp;		}		public function getRevive():Boolean{			return canRevive		}	}	}